;Hola Mundo
section .data
	msg db 'Hola Zavala', 0xa
section .text
	global main
main:
	mov eax, 0x4	;tipo de syscall (sys write en este caso)
	mov ebx, 0x1	;describe el archivo, si es a consola...
	mov ecx, msg	;variable a imprimir
	mov edx, 11	;cantidad de bytes a imprimir
	int 0x80	;es como llamar a la funcion
	;ahora salimos del programa
	mov eax, 0x1	;codigo de cierre de syscall
	xor ebx, ebx	;comparacion la direccion rdi para que el valor sea 0 es mas eficiente que mov rdi, 0
	int 0x80	;ahora llamamos para cerrar
